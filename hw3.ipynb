  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import os\n",
    "def read(path = 'assignment3_data/fruit/'):\n",
    "    # use os fucntions \n",
    "    myfiles = os.listdir(path)\n",
    "    df = pd.DataFrame()\n",
    "    #find each file and iterated \n",
    "    for f in myfiles:\n",
    "        data = pd.read_excel(path + f)\n",
    "        is_fresh = data.iloc[:,0].str.contains(\"Fresh\").fillna(False)\n",
    "        data = data.loc[is_fresh, :]\n",
    "        \n",
    "        #fix columns\n",
    "        if len(data.columns) > 7:\n",
    "            data = data.drop(labels = data.columns[[7,8]],  axis= 1)\n",
    "        data = data.drop(labels = data.columns[[2,5]], axis= 1)\n",
    "\n",
    "        data.columns = [\"form\", \"price_per_pound\", \"yield\",\"lb_per_cup\",\"price_per_cup\"]\n",
    "        #inserting data\n",
    "        data.insert(0,\"food\", f.split(\".\")[0].split(\"/\")[-1])\n",
    "        #splitting data\n",
    "        data.insert(0,\"type\", path.split(\".\")[0].split(\"/\")[1])\n",
    "        #Appending data in one data frame\n",
    "        df = df.append(data)\n",
    "            \n",
    "    return (df)\n"
   ]
