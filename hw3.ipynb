    "\n",
    "'''#How to Split \n",
    "filename = \"assignment3_data/fruit/grapes.xlsx\"\n",
    "#[0] to get the first element then [-1] to get the last element which is \"grapes\"\n",
    "grapes=filename.split(\".\")[0].split(\"/\")[-1] \n",
    "grapes'''"
   ]
